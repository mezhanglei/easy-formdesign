{
  "name": "@easy-formdesign/form",
  "version": "0.0.1",
  "description": "simple form component",
  "author": "mezhanglei <496623925@qq.com>",
  "homepage": "https://github.com/mezhanglei/easy-formdesign#readme",
  "license": "ISC",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "./lib/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib",
    "README.md",
    "README_CN.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mezhanglei/easy-formdesign.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build:typings": "tsc -d --emitDeclarationOnly --target esnext --removeComments false --allowJs false --skipLibCheck true --suppressImplicitAnyIndexErrors true --declarationDir ./lib",
    "build": "cross-env NODE_ENV=prod webpack --progress --config ./webpack/webpack.prod.js",
    "stats": "cross-env NODE_ENV=prod webpack --progress --config ./webpack/webpack.prod.js --json > stats.json",
    "start": "cross-env NODE_ENV=dev webpack serve --config ./webpack/webpack.dev.js",
    "lint": "eslint --ext=.js,.ts,.jsx,.tsx src/",
    "lint:fix": "eslint --fix --ext=.js,.ts,.jsx,.tsx src/",
    "csslint": "stylelint src/**/*.{css,scss,less}",
    "csslint:fix": "stylelint src/**/*.{css,scss,less} --fix"
  },
  "dependencies": {
    "@tippyjs/react": "^4.2.6",
    "antd": "^5.11.1",
    "classnames": "^2.3.2",
    "copy-anything": "^3.0.5",
    "history": "^5.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-fast-compare": "^3.2.2",
    "react-router-dom": "^6.19.0"
  }
}
